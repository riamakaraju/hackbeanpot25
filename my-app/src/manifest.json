{
    "manifest-version": 3,
    "name": "G-Lock",
    "description": "Block distracting websites",
    "version": "1.0",
    "action" : {
        "default_popup": "popup/popup.html"
        
    },

    "background": {
        "service_worker": "background.js",
        "type": "module"
    },

    "permissions": [
        "storage"
    ]
}